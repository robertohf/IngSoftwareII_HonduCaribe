
<div class="form-group">
  <%= f.label :school_name, "Institucion" %>
  <%= f.text_field :school_name, class: "form-control", placeholder: "Institucion" %>
</div>
<div class="form-group">
  <%= f.label :title_description, "Titulo" %>
  <%= f.text_field :title_description, class: "form-control", placeholder: "Titulo" %>
</div>
<div class="form-group">
  <%= f.label :year_start, "Fecha de Inicio" %>
  <%= f.date_field :year_start, class: "form-control" %>
</div>
<div class="form-group">
  <%= f.label :year_end, "Fecha de Culminacion" %>
  <%= f.date_field :year_end, class: "form-control" %>
</div>
<div class="field">
  <%= f.label :completed, "Completado" %> <br />
  <%= f.check_box :completed %> <br />
</div>
<div class="form-group">
  <%= f.label :document, "Constancia" %>
  <%= f.file_field :document %>
</div>
