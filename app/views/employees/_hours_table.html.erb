<table class="table table-hover">
  <thead style="font-size: 18px; font-family: Fjalla One, sans-serif; color: #00263D">
    <tr>
      <th>Codigo de Empleado</th>
      <th>Nombre</th>
      <th>Normal</th>
      <th>25%</th>
      <th>50%</th>
      <th>75%</th>
      <th>100%</th>
      <th>Total Horas Extra</th>
      <th>VAC</th>
      <th>ASP</th>
      <th>ACP</th>
    </tr>
  </thead>
  <tbody>
    <%- employees.each do |f| %>
      <%- hours = f.get_hours(time_in.to_time.beginning_of_day, time_out.to_time.end_of_day) %>
      <tr>
        <td><%= f.work_id%></td>
        <td><%= link_to f.name, employee_path(f) %></td>
        <td><%= Time.at(hours["normal"] * 60).utc.strftime("%H:%M") %></td>
        <td><%= Time.at(hours["25"]*60).utc.strftime("%H:%M") %></td>
        <td><%= Time.at(hours["50"]*60).utc.strftime("%H:%M") %></td>
        <td><%= Time.at(hours["75"]*60).utc.strftime("%H:%M") %></td>
        <td><%= Time.at(hours["100"]*60).utc.strftime("%H:%M") %></td>
        <td><%= hours['Extra']/60 %></td>
        <td><%= hours["VAC"] %></td>
        <td><%= hours["ASP"] %></td>
        <td><%= hours["ACP"] %></td>
      </tr>
    <%- end %>
  </tbody>
</table>
