
<div class="right_col" role="main">
  <div class="">
    <div class="page-title not-printable">
      <div class="title_left">
        <h3>HonduCaribe</h3>
      </div>

      <div class="title_right not-printable">
        <div class="col-md-5 col-sm-5 col-xs-12 form-group pull-right top_search not-printable">
            <%= form_tag employees_path, remote: true, method: :get, class: "js-employees-filter form-inline not-printable" do %>
              <%=text_field_tag :q, params[:q], placeholder: "Buscar...", class: "form-control not-printable", display: "inline-block" %>
                <%= button_tag type: "submit", class: "btn btn-default", display: "inline-block" do %>
                  <span class="input-group-btn"> Ir! </span>
                <%end%>
      	   <% end %>
        </div>
      </div>
    </div>
    <div class="clearfix"></div>

    <div class="row">

      <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel" style="height:600px; ">
          <div class="x_title">
            <h2>Empleados con hijos menores de 18</h2>
            <div class="clearfix"></div>
          </div>
          <div style="float: center; border-left: 500px">
            <div class="js-employees-table">
              <%= render partial: "table", locals: { employees: @employees} %>
            </div>
          </div>
          <div class="not-printable">
            <%= link_to "Cancelar", employees_path%>

          </div>
        </div>
      </div>
    </div>
  </div>
</div>
