<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã—</span></button>
        <h4 class="modal-title" id="myModalLabel">Editar Experiencia Laboral</h4>
        </div>
        <%= form_for [@employee, @work_exp] do |f| %>
        <div class="modal-body">
        <div class="form-group">
            <%= f.label :company_name, "Institucion" %>
            <%= f.text_field :company_name, class: "form-control", placeholder: "Lugar" %>
        </div>
        <div class="form-group">
            <%= f.label :position, "Titulo" %>
            <%= f.text_field :position, class: "form-control", placeholder: "Posicion" %>
        </div>
        <div class="form-group">
            <%= f.label :year_start, "Fecha de Inicio" %>
            <%= f.date_field :year_start, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= f.label :year_end, "Fecha de Culminacion" %>
            <%= f.date_field :year_end, class: "form-control" %>
        </div>
        <div class="form-group">
            <%= f.label :constancia, "Constancia" %>
            <%= f.file_field :constancia %>
        </div>
        </div>
        <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
        <%= f.submit "Actualizar" , class: "btn btn-primary"%>
        </div>
        <%- end %>
    </div>
</div>